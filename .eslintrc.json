{
  "env": {
    "node": true,
    "es2022": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["error", "ignorePackages"],
    "quotes": ["error", "double"],
    "no-console": ["error", { "allow": ["warn", "error", "info"] }]
  }
}
